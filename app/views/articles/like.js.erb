var article_id = ".article-id-" + "<%= @article.id %>";
var article_count = ".article-count-" + "<%= @article.id %>";
$(article_id).text("<%= @article.like_count %>");
<% if @article.liked_by?(current_user) %>
  $(article_count).removeClass('fa-heart-o').addClass('fa-heart');
<% else %>
  $(article_count).removeClass('fa-heart').addClass('fa-heart-o');
<% end %>
