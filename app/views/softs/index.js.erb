$("<%= j render(:partial => 'index', :object => @softs) %>")
  .appendTo($(".infinite-table"));

// Update pagination link
<% if @softs.last_page? %>
  $('.my-pagination').html("");
  $('.pagination-text').html("没有更多了");
<% else %>
  $('.my-pagination')
    .html("<%=j link_to_next_page(@softs, '下一页', :remote => true) %>");
<% end %>
